{
    "name": "akoteam/bitrah",
    "description": "bitrah gateway package for laravel",
    "type": "library",
    "version": "1.0.0",
    "license": "MIT",
    "keywords": [
        "laravel",
        "laravel-gateway",
        "laravel-bitrah",
        "bitrah",
        "bitrah-gateway"
    ],
    "autoload": {
        "psr-4": {
            "Akoteam\\Bitrah\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Akoteam\\Bitrah\\BitrahServiceProvider"
            ],
            "aliases": {
                "Bitrah": "Akoteam\\Bitrah\\Facades\\Bitrah"
            }
        }
    },
    "homepage": "https://github.com/umidzarei/Bitrah",
    "authors": [
        {
            "name": "Omid Zarei",
            "email": "umidzarei@gmail.com"
        },
        {
            "name": "Farhad Rad",
            "email": "farhad.rad.official@gmail.com"
        },
        {
            "name": "Ali Seyed",
            "email": "ialiseyed@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {},
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish --tag={bitrah_config} --force"
        ]
    }
}